<% unless @edit_mode: %>
  <li class="edit_tape_button edit_button" data-id="<%=@model.get("tape").get("_id")%>"></li>
  <li class="remove_tape_button edit_button" data-id="<%=@model.get("tape").get("_id")%>"></li>
  <li class="star_tape_button edit_button" data-id="<%=@model.get("tape").get("_id")%>"></li>
<% else: %>
  <li class="undo_tape_button edit_button" data-id="<%=window.lastTape%>"></li>
<% end %>
