<li class="user_box"> 

  <div class="icon">
    <img src="http://<%= @model.get("picture")%>">
  </div>

  <div class="headline_box">
    
    <div class="line">
      <%= @model.get("name")%> 
    </div>
    <div class="line">
      <%= @model.get("email")%> 
    </div>
  
  </div>
  <div class="headline_box last">
    
    <div class="line">
      <%= @model.get("user_agent")%> 
    </div>
    <div class="line">
      <%= @model.get("language")%> 
    </div>
  
  </div>
</li>

